<Window x:Class="UI.Desktop.Checkout.CheckoutWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Kassa" Height="470" Width="611" ResizeMode="NoResize"
        ShowInTaskbar="False"
        WindowStartupLocation="CenterOwner">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="180"/>
            <ColumnDefinition Width="1*"/>
        </Grid.ColumnDefinitions>
        <Grid x:Name="PageGrid" VerticalAlignment="Stretch" Margin="0,0,0,0" Grid.Column="1"/>
        <Grid x:Name="wizardSteps">
            <Grid HorizontalAlignment="Stretch" Margin="0,50,0,0" VerticalAlignment="Top" Height="60">
                <Label Content="Label" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
                <Image HorizontalAlignment="Right" Height="60" Margin="0,0,10,0" VerticalAlignment="Top" Width="60" Source="/UI.Desktop;component/Resources/Icons/user.png"/>
            </Grid>
            <Label Content="Inloggningstatus" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>

        </Grid>
    </Grid>
</Window>
